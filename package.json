{
  "name": "significa.co",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "prebuild": "npm run datasources",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "sb:login": "npx storyblok@latest logout && npx storyblok login",
    "sb:pull": "npx storyblok@latest pull-components --space=198185",
    "sb:types": "npx storyblok-generate-ts@latest source=./components.198185.json target=./src/types/bloks.d.ts && prettier ./src/types/bloks.d.ts --write",
    "sb": "npm run sb:pull && npm run sb:types",
    "datasources": "svelte-kit sync && npx ts-node@latest --project ./node.tsconfig.json ./scripts/datasources.ts"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.0-beta.49",
    "@storybook/addon-essentials": "^7.0.0-beta.49",
    "@storybook/addon-interactions": "^7.0.0-beta.49",
    "@storybook/svelte": "^7.0.0-beta.49",
    "@storybook/sveltekit": "^7.0.0-beta.49",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.6.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "prismjs": "^1.29.0",
    "storybook": "^7.0.0-beta.49",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-mkcert": "^1.13.0",
    "vitest": "^0.28.5"
  },
  "type": "module",
  "dependencies": {
    "@significa/svelte-ui": "^0.0.7",
    "@storyblok/js": "^2.0.15"
  }
}
