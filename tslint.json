{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-a11y"
  ],
  "rules": {
    "member-access": [true, "no-public"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "interface-over-type-literal": false,
    "jsx-boolean-value": ["never"]
  }
}
